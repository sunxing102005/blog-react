{
    "compilerOptions": {
        "jsx": "react",
        "lib": ["es6", "dom"],
        "rootDir": "src",
        "module": "commonjs",
        "target": "es5",
        "sourceMap": true,
        "moduleResolution": "node",
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "experimentalDecorators": true
    },
    "include": ["./src/"],
    "exclude": ["node_modules"],
    "extends": "./node_modules/react-redux-typescript-scripts/tsconfig.json"
}
